{
    "name": "Image Replacer",
    "description": "Replace all images with whatever you want.",
    "author": "Luigi's Hat",
    "manifest_version": 2,
    "version": "1.743",
    "browser_action": {
        "default_title": "Image Replacer",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "run_at": "document_end",
            "js": ["content.js"],
            "all_frames": true
        }
    ],
    "permissions": ["storage"],
    "short_name": "Image Replacer"
}