{
  "manifest_version": 2,

  "name": "What-If Score",
  "description": "This extension will analyze Axis and simulate the Canvas What If Score System",
  "version": "1.2343453",
  "author": "Caleb Behunin",
  "permissions": [
   "activeTab",
   "declarativeContent",
   "storage"
 ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
      "default_title": "What-If Score",
      "js": "popup.js"
  },
  "content_scripts": [
    {
      "matches": ["https://oakschristian.myschoolapp.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}
